{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "module": "commonjs",
    "target": "es6",
    "noResolve": false,
    "experimentalDecorators": true,
    "declaration": true
  },
  "include": ["./src/**/*"]
}
